drop keyspace if exists TEST;
create keyspace if not exists TEST with replication = { 'class' : 'SimpleStrategy', 'replication_factor' : 1 };
drop table if exists TEST.SIMPLE;
create table if not exists TEST.SIMPLE( Uuid uuid, Bool boolean, Int8 tinyint, Uint8 tinyint, Int16 smallint, Uint16 smallint, Int32 int, Uint32 int, Int64 bigint, Uint64 bigint, Float32 float, Float64 double, String text, Name text, Date timestamp, DateTime timestamp, Blob blob, primary key( ( Uuid, Bool ), Int8 ) );
create index on TEST.SIMPLE ( Uint8 );
drop table if exists TEST.COMPOUND;
create table if not exists TEST.COMPOUND( Id int, Location text, Name tuple<text,text>, NameSet set<frozen<tuple<text,text>>>, PhoneList list<text>, EmailSet set<text>, CompanyMap map<text,text>, SimpleDate timestamp, SimpleDateMap map<frozen<tuple<text,text>>,timestamp>, SimpleDateSet set<timestamp>, SimpleDateList list<timestamp>, NameSetMap map<timestamp,frozen<set<tuple<text,text>>>>, primary key( Id ) );
drop table if exists TEST.VALUE;
create table if not exists TEST.VALUE( Id bigint, Uuid uuid, Name text, Integer int, Text text, primary key( Id ) );
create index on TEST.VALUE ( Uuid );
drop table if exists TEST.VALUE_DATA;
create table if not exists TEST.VALUE_DATA( Uuid uuid, ValueId bigint, ValueUuid uuid, primary key( Uuid ) );
